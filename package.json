{
  "name": "@vcsuite/ui-components",
  "version": "0.1.5",
  "type": "module",
  "description": "Vuetify based UI components.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"{src,tests}/**/*.{js,vue}\"",
    "docs": "jsdoc -c build/jsdocConfig.json"
  },
  "files": [
    "index.js",
    "src/"
  ],
  "peerDependencies": {
    "@vcsuite/logger": "^1.0.1",
    "@vcsuite/check": "^1.1.1",
    "@mdi/font": "^5.9.55",
    "@vue/composition-api": "^1.3.3",
    "fuse.js": "^6.4.6",
    "rxjs": "^6.6.6",
    "vue": "^2.6.11",
    "vuetify": "^2.5.3"
  },
  "devDependencies": {
    "@vcsuite/eslint-config": "^2.0.3",
    "@vue/compiler-sfc": "^3.2.26",
    "eslint": "^8.1.0",
    "jsdoc": "^3.6.7",
    "jsdoc-vuejs": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "eslintConfig": {
    "extends": [
      "@vcsuite/eslint-config/vue"
    ],
    "plugins": [
      "jsdoc",
      "vue"
    ],
    "rules": {
      "jsdoc/check-tag-names": [
        "error",
        {
          "definedTags": [
            "vue-prop",
            "vue-data",
            "vue-computed",
            "vue-event"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "author": "virtualcitysystems GmbH",
  "license": "ISC"
}
